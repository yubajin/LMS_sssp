package com.jxufe.lms.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="booklists")
public class Book {
	@Id
	private String bookid;
	private String bookname;
	
	private String librarybelong;
	private String bookbelong;
	
	private String bookimg;
	private long librarycanborrow;
	
	private long readednumbers;
	private String summary;
	
	private long librarycollection;
	private String author;
	public String getBookid() {
		return bookid;
	}
	public void setBookid(String bookid) {
		this.bookid = bookid;
	}
	public String getBookname() {
		return bookname;
	}
	public void setBookname(String bookname) {
		this.bookname = bookname;
	}
	public String getLibrarybelong() {
		return librarybelong;
	}
	public void setLibrarybelong(String librarybelong) {
		this.librarybelong = librarybelong;
	}
	public String getBookbelong() {
		return bookbelong;
	}
	public void setBookbelong(String bookbelong) {
		this.bookbelong = bookbelong;
	}
	public String getBookimg() {
		return bookimg;
	}
	public void setBookimg(String bookimg) {
		this.bookimg = bookimg;
	}
	public long getLibrarycanborrow() {
		return librarycanborrow;
	}
	public void setLibrarycanborrow(long librarycanborrow) {
		this.librarycanborrow = librarycanborrow;
	}
	public long getReadednumbers() {
		return readednumbers;
	}
	public void setReadednumbers(long readednumbers) {
		this.readednumbers = readednumbers;
	}
	public String getSummary() {
		return summary;
	}
	public void setSummary(String summary) {
		this.summary = summary;
	}
	public long getLibrarycollection() {
		return librarycollection;
	}
	public void setLibrarycollection(long librarycollection) {
		this.librarycollection = librarycollection;
	}
	public String getAuthor() {
		return author;
	}
	public void setAuthor(String author) {
		this.author = author;
	}
	
	
}
